{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "outputDirectory": "snap/client/.next",
  "monorepoManager": "npm",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "snap/server/server.js"
    }
  ],
  "functions": {
    "snap/server/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}